<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:interpolator="@android:interpolator/decelerate_quint">

    <alpha android:fromAlpha="0.25"
        android:toAlpha="1"
        android:duration="1000"/>

    <scale android:pivotX="50%"
        android:pivotY="50%"
        android:fromXScale="60%"
        android:fromYScale="60%"
